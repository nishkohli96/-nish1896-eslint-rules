{
  "name": "@nish1896/eslint-config",
  "version": "2.0.3",
  "description": "eslint and stylistic rules for eslint 8.56.0 or below to help you focus more on the code logic, while they take care of the code formatting",
  "author": "Nishant Kohli",
  "exports": {
    "./js": "./lib/js/index.js",
    "./react": "./lib/react/index.js"
  },
  "scripts": {
    "test": "eslint __tests__",
    "fix": "eslint --fix __tests__",
    "link:local": "npm link",
    "unlink:local": "npm unlink @nish1896/eslint-config",
    "publish:manual": "npm publish --access public"
  },
  "keywords": [
    "@nish1896",
    "@nish1896/eslint-config",
    "eslint",
    "eslintconfig",
    "eslint-config",
    "eslint-config-jsx",
    "eslint-config-react",
    "eslit-react",
    "lint-config",
    "lint-rules",
    "react-eslint"
  ],
  "homepage": "https://github.com/nishkohli96/eslint-config#readme",
  "repository": {
    "url": "git+https://github.com/nishkohli96/eslint-config.git"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^1.7.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "typescript-eslint": "^7.11.0"
  },
  "engines": {
    "node": ">=14"
  },
  "peerDependencies": {
    "eslint": ">=7",
    "typescript": ">=4"
  },
  "license": "ISC"
}
