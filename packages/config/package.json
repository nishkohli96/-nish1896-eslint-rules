{
  "name": "@nish1896/eslint-config",
  "version": "2.1.0",
  "description": "eslint and stylistic rules to help you focus more on the code logic, while they take care of the code formatting",
  "author": "Nishant Kohli",
  "exports": {
    "./v8/js": "./lib/v8/js/index.js",
    "./v8/react": "./lib/v8/react/index.js",
    "./v9/js": "./lib/v9/js/index.mjs",
    "./v9/react": "./lib/v9/react/index.mjs"
  },
  "scripts": {
    "test": "eslint __tests__",
    "fix": "eslint --fix __tests__",
    "link:local": "npm link",
    "unlink:local": "npm unlink @nish1896/eslint-config",
    "publish:manual": "npm publish --access public"
  },
  "keywords": [
    "@nish1896",
    "@nish1896/eslint-config",
    "eslint",
    "eslintconfig",
    "eslint-config",
    "eslint-config-jsx",
    "eslint-config-react",
    "eslit-react",
    "lint-config",
    "lint-rules",
    "react-eslint"
  ],
  "homepage": "https://github.com/nishkohli96/eslint-config#readme",
  "repository": {
    "url": "git+https://github.com/nishkohli96/eslint-config.git"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^1.7.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2"
  },
  "engines": {
    "node": ">=14"
  },
  "peerDependencies": {
    "eslint": ">=7",
    "typescript": ">=4"
  },
  "license": "ISC"
