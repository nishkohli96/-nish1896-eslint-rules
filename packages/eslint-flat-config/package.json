{
  "name": "@nish1896/eslint-flat-config",
  "version": "1.0.0",
  "description": "eslint and stylistic rules for eslint v9 and above to help you focus more on the code logic, while they take care of the code formatting",
  "author": "Nishant Kohli",
  "exports": {
    "./js": "./lib/js/index.mjs",
    "./react": "./lib/react/index.mjs"
  },
  "scripts": {
    "test": "eslint __tests__",
    "fix": "eslint --fix __tests__",
    "link:local": "npm link",
    "unlink:local": "npm unlink @nish1896/eslint-flat-config",
    "publish:manual": "npm publish --access public"
  },
  "keywords": [
    "@nish1896",
    "@nish1896/eslint-config",
    "@nish1896/eslint-config-flat",
    "@nish1896/eslint-flat-config",
    "eslint",
    "eslintconfig",
    "eslint-config",
    "eslint-config-flat",
    "eslint flat config",
    "flat eslint config",
    "eslint-config-jsx",
    "eslint-config-react",
    "eslit-react",
    "lint-config",
    "lint-rules",
    "react-eslint"
  ],
  "homepage": "https://github.com/nishkohli96/eslint-config#readme",
  "repository": {
    "url": "git+https://github.com/nishkohli96/eslint-config.git"
  },
  "dependencies": {
    "@eslint/js": "^9.2.0",
    "@stylistic/eslint-plugin": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "typescript-eslint": "^7.9.0"
  },
  "engines": {
    "node": ">=18.18.0 || >=20.9.0 || >=21"
  },
  "peerDependencies": {
    "eslint": ">=9",
    "typescript": ">=4"
  },
